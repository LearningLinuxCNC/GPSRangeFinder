<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android" >
        </shape>
    </item>
    <item
        android:left="2dp"
        android:right="2dp"
        android:top="5dp"
        android:bottom="5dp">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" >
            <solid
                android:color="#1eff00"/>
            <corners
                android:radius="4dp"/>
            <padding
                android:left="10dp"
                android:right="10dp"
                android:top="10dp"
                android:bottom="10dp"/>
            <stroke
                android:width="1dp"
                android:color="#aeaeae"/>
        </shape>
    </item>
</layer-list>